  "editor.tokenColorCustomizations": {
      "textMateRules": [
                {
      "scope": [
        "meta.function-call.generic.python",
        "variable.language.super.js",
        "variable.language.super.ts",
        "variable.language.super.tsx",
      ],
      "settings": {
      "foreground": "#286EDC",
      "fontStyle": "bold"
                  }
                },
      {
        "scope": [
          "variable.parameter.function-call",
          "variable.parameter.function.language.python"
        ],
        "settings": {
            // "foreground": "#FF6E00",
            "foreground": "#F39C12",
        }
        },                
      {
      "scope": "keyword.operator",
      "settings": {
          "foreground": "#E10F86",
      }
      },      
      {
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.ts",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.html",
      ],
      "settings": {
          "foreground": "#F39C12",
      }
      },
      {
        "scope": "variable.other",
        "settings": {
            "foreground": "#B9B4B4", 
            // "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "entity.name.type.class",
          "storage.modifier.async.js",
          "variable.language.super.js",
          "support.function.console.js",         
          "storage.modifier.async.tsx",
          "storage.modifier.async.ts",
          "variable.language.super.tsx",
          "variable.language.super.ts",
          "support.function.console.tsx",
          "support.function.console.ts"
          // "storage.type.js"
        ],
        "settings": {
            // "foreground": "#E10F86", 
            "fontStyle": "bold"
        }
      },      
      {
      "scope": "storage.type",
      "settings": {
          "foreground": "#B841AD",
          // "fontStyle": "bold"
      }
      },
      {
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#B5D005",
        }
      },       
      {
        "scope": [
          "variable.other.property",
          "meta.attribute.python",
          // "punctuation.separator.key-value",
          // "meta.object-literal.key.js",
          "variable.other.object.property.js",
          "variable.other.object.property.tsx",
          "variable.other.object.property.ts",
          "support.class.console.js",
          "support.class.console.tsx",
          "support.class.console.ts",
          "entity.other.inherited-class",
        ],
        "settings": {
          "foreground": "#E74C3C",
          // "fontStyle": "bold"
        }
      },      
      {
        "scope": [
          "meta.object-literal.key.js",
          "meta.object-literal.key.ts",
          "meta.object-literal.key.tsx",
          "meta.jsx.children",
          "punctuation.definition.tag.end",
          "punctuation.definition.tag.begin",
        ],
        "settings": {
          "foreground": "#B9B4B4",
        }
      },         
      {
        "scope": [
          "entity.name.function", 

        ],
        "settings": {
          "foreground": "#3884F9",
          // "fontStyle": "bold"
        }
      }, 
      {
        "scope": "entity.other.inherited-class",
        "settings": {
          // "foreground": "#E74C3C",
          "foreground": "#932CF5",
          "fontStyle": "", 
        }
      },  
      {
        // "scope": "support.type",
        "scope": [
          "variable.other.readwrite.js",
          "variable.other.readwrite.ts",
          "variable.other.readwrite.tsx",
        ],

        "settings": {
          // "foreground": "#E74C3C",
          // "foreground": "#F14D4D",
          // "fontStyle": "bold", 
        }
      },               
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#B5D005",
          // "fontStyle": "bold"
        }
      },       
      {
        "scope": "punctuation.separator.key-value",
        "settings": {
          "foreground": "#E10F86",
          "fontStyle": "bold"
        }
      },   
      {
        "scope": [
          "meta.template.expression.js",
          "meta.template.expression.ts",
          "meta.template.expression.tsx",
        ],
        "settings": {
          "foreground": "#F35A12",
          // "fontStyle": "bold"
        }
      },      
      {
        "scope": "entity.name.type",
        "settings": {
          "foreground": "#D4AC0D",
          "fontStyle": "bold"
        }
      },  
      {
        "scope": "support.constant",
        "settings": {
          "foreground": "#8DC760",
          "fontStyle": "bold"
        }
      },          
      {
        "scope": [
          "support.function",
          // "support.function.js",
          // "support.function.ts",
          // "support.function.tsx",
          // "support.function.math.tsx",
          // "support.function.math.jsx",
          // "support.function.math.js",    
          // "support.function.console.js",
          // "support.function.console.ts",
          // "support.function.console.tsx",   
          "support.function.builtin",             
        ],
        "settings": {
          "foreground": "#2FBDC2",
          // "fontStyle": "bold"
        }
      },      
      { 
        "scope": "support.class.console.js",
        "settings": {
          // "foreground": "#79C70E",
          "fontStyle": ""
        }
      },      
      ],

