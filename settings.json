  "editor.tokenColorCustomizations": {
      "textMateRules": [
                {
      "scope": [
        "meta.function-call.generic.python",
        "variable.language.super.js"
      ],
      "settings": {
      "foreground": "#286EDC",
      "fontStyle": "bold"
                  }
                },
      {
        "scope": [
          "variable.parameter.function-call",
          "variable.parameter.function.language.python"
        ],
        "settings": {
            // "foreground": "#FF6E00",
            "foreground": "#F39C12",
        }
        },                
      {
      "scope": "keyword.operator",
      "settings": {
          "foreground": "#E10F86",
      }
      },      
      {
      "scope": "entity.other.attribute-name.js",
      "settings": {
          "foreground": "#F39C12",
      }
      },
      {
        "scope": "variable.other",
        "settings": {
            "foreground": "#B9B4B4", 
            // "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "entity.name.type.class",
          "storage.modifier.async.js",
          "variable.language.super.js",
          // "storage.type.js"
        ],
        "settings": {
            // "foreground": "#E10F86", 
            "fontStyle": "bold"
        }
      },      
      {
      "scope": "storage.type",
      "settings": {
          "foreground": "#B841AD",
          // "fontStyle": "bold"
      }
      },
      {
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#B5D005",
        }
      },       
      {
        "scope": [
          "variable.other.property",
          "meta.attribute.python",
          "punctuation.separator.key-value",
          "meta.object-literal.key.js",
        ],
        "settings": {
          "foreground": "#E74C3C",
          // "fontStyle": "bold"
        }
      },      
      {
        "scope": "string.quoted.docstring.multi.python",
        "settings": {
          // "foreground": "#606060",
          // "fontStyle": "bold"
        }
      },         
      {
        // "scope": "variable.other.readwrite.js",
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#3884F9",
          // "fontStyle": "bold"
        }
      }, 
      {
        "scope": "entity.other.inherited-class",
        "settings": {
          "foreground": "#E74C3C",
          "fontStyle": "", 
        }
      },  
      {
        "scope": "support.type",
        "settings": {
          // "foreground": "#11B1B8",
          // "fontStyle": "bold", 
        }
      },               
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#B5D005",
          // "fontStyle": "bold"
        }
      },       
      {
        "scope": "punctuation.separator.key-value",
        "settings": {
          // "foreground": "#B5D005",
          "fontStyle": "bold"
        }
      },   
      {
        "scope": "entity.name.type.class",,
        "settings": {
          "foreground": "#D4AC0D",
          "fontStyle": "bold"
        }
      },      

      ],
        

